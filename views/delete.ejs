	<li><a href="/">Home</a></li>
   
    <li class="active">Delete User Credentials </a></li>
</ol>

<br/>
<%- partial ('partials/statusmessage.ejs') %>
<h2>Users</h2>
<table class="table table-striped table-bordered">
  <tr>
    <th>ID</th>
    <th>User</th>
    <th>Name</th>
    <th>DOB</th>
   
  </tr>
  <% if(typeof users != "undefined" && users.length > 0){ %>
    <% users.forEach(function(user){ %>
      <tr>
        <td><%= user.id %></td>
        <td><%= user.user %></td>
        <td><%= user.name %></td>
        <td><%= user.dob %></td>
        
      </tr>
    <% }) %>
  <% }else{ %>
      <tr>
        <td colspan="8">no records found</td>
      </tr>
  <% } %>
  
</table>


<h2>Delete User</h2>
<form action="/delete" method="POST" class="form-horizontal">
 <div class="form-group">
   <label for="id" class="col-sm-2 control-label">User ID #</label>
   <div class="col-sm-10">
     <input type="number" class="form-control" id="id" name="id" placeholder="User ID #">
   </div>
 </div>
  <div class="form-group">
    <div class="col-sm-offset-2 col-sm-10">
      <button type="submit" class="btn btn-default">Delete User</button>
    </div>
  </div>
</form>